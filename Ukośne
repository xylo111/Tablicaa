#include <iostream>
#include <ctime>
#include <cstdlib>

using namespace std;
int main()
{
int sumap,sumal,n;
int suma=0;

    cout<<"Podaj rozmiar tablicy: ";
    cin>>n;
    
    int tab[n][n];
    srand(time(NULL));
    
    for (int i=0;i<n;i++)
    {
        for (int j=0;j<n;j++)
        {
            tab[i][j]=rand()%89+10;
            cout<<tab[i][j]<<" | ";
        {
        if(i==j)
        {
             sumal += tab[i][j];
        }
        if((i+j) == n-1)
        {
             sumap += tab[i][j];
        }
        }
    }
        cout<<endl;
    }
cout << "Suma lewej ukosnej: "<< sumal << endl;
cout << "Suma prawej ukosnej: "<< sumap << endl;
return 0;
}
